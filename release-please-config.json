{
  "bootstrap-sha": "5f630bb5305e826037104de97ec2b61eaab3f96a"
  "pull-request-title-pattern": "chore: release${component} ${version}",
  "group-pull-request-title-pattern": "chore: release ${branch}",
  "packages": {
    "api": {
      "releaseType": "simple",
      "draft": false,
      "prerelease": false,
      "bumpMinorPreMajor": false,
      "bumpPatchForMinorPreMajor": false,
      "changelogPath": "CHANGELOG.md",
      "versioning": "default"
    },
    "misc": {
      "releaseType": "simple",
      "draft": false,
      "prerelease": false,
      "bumpMinorPreMajor": false,
      "bumpPatchForMinorPreMajor": false,
      "changelogPath": "CHANGELOG.md",
      "versioning": "default"
    },
    "web": {
      "releaseType": "simple",
      "draft": false,
      "prerelease": true,
      "bumpMinorPreMajor": false,
      "bumpPatchForMinorPreMajor": false,
      "changelogPath": "CHANGELOG.md",
      "versioning": "default"
    }
  }
  "plugins": ["cargo-workspace"]
}
